<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2005 rel. 3 U (http://www.altova.com) by Stefan Hoops (VBI) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:element name="COPASI" id="copasi.COPASI">
    <xs:annotation>
      <xs:documentation>The root of a COPASI xml file.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ListOfFunctions" id="copasi.ListOfFunctions" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The list of functions used by the current model. If there are no functions in the model this element must not occur.</xs:documentation>
          </xs:annotation>
          <xs:complexType mixed="true">
            <xs:sequence>
              <xs:element name="Function" type="cFUNCTION" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
          <xs:unique name="FunctionName">
            <xs:selector xpath="Function"/>
            <xs:field xpath="@name"/>
          </xs:unique>
        </xs:element>
        <xs:element name="Model" id="copasi.Model" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Comment" type="cCOMMENT" id="copasi.Model.Comment" minOccurs="0"/>
              <xs:element name="ListOfCompartments" id="copasi.Model.ListOfCompartments" minOccurs="0">
                <xs:complexType mixed="true">
                  <xs:sequence>
                    <xs:element name="Compartment" id="copasi.Model.Compartment" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="key" type="cCOMPARTMENT_KEY" use="required" id="copasi.Model.Compartment.key"/>
                        <xs:attribute name="name" type="xs:string" use="required" id="copasi.Model.Compartment.name"/>
                        <xs:attribute name="stateVariable" type="cSTATE_VARIABLE_KEYREF" use="required" id="copasi.Model.Compartment.stateVariable"/>
                        <xs:attribute name="sbmlid" type="xs:string" use="optional" id="copasi.Model.Compartment.sbmlid"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
                <xs:unique name="CompartmentName">
                  <xs:selector xpath="Compartment"/>
                  <xs:field xpath="@name"/>
                </xs:unique>
              </xs:element>
              <xs:element name="ListOfMetabolites" id="copasi.Model.ListOfMetabolites" minOccurs="0">
                <xs:complexType mixed="true">
                  <xs:sequence>
                    <xs:element name="Metabolite" id="copasi.Model.Metabolite" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="key" type="cMETABOLITE_KEY" use="required" id="copasi.Model.Metabolite.key"/>
                        <xs:attribute name="name" type="xs:string" use="required" id="copasi.Model.Metabolite.name"/>
                        <xs:attribute name="compartment" type="cCOMPARTMENT_KEYREF" use="required" id="copasi.Model.Metabolite.compartment"/>
                        <xs:attribute name="status" use="required">
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:enumeration value="fixed"/>
                              <xs:enumeration value="variable"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="stateVariable" type="cSTATE_VARIABLE_KEYREF" use="required" id="copasi.Model.Metabolite.stateVariable"/>
                        <xs:attribute name="sbmlid" type="xs:string" use="optional" id="copasi.Model.Metabolite.sbmlid"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
                <xs:unique name="MetaboliteName">
                  <xs:selector xpath="Metabolite"/>
                  <xs:field xpath="@name"/>
                  <xs:field xpath="@compartment"/>
                </xs:unique>
              </xs:element>
              <xs:element name="ListOfModelValues" id="copasi.Model.ListOfModelValues" minOccurs="0">
                <xs:complexType mixed="true">
                  <xs:sequence>
                    <xs:element name="ModelValue" id="copasi.Model.ModelValue" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="key" type="cMODELVALUE_KEY" use="required" id="copasi.Model.ModelValue.key"/>
                        <xs:attribute name="name" type="xs:string" use="required" id="copasi.Model.ModelValue.name"/>
                        <xs:attribute name="status" use="required">
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:enumeration value="fixed"/>
                              <xs:enumeration value="variable"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="stateVariable" type="cSTATE_VARIABLE_KEYREF" use="required" id="copasi.Model.ModelValue.stateVariable"/>
                        <xs:attribute name="sbmlid" type="xs:string" use="optional" id="copasi.Model.Metabolite.sbmlid"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
                <xs:unique name="MetaboliteName">
                  <xs:selector xpath="Metabolite"/>
                  <xs:field xpath="@name"/>
                  <xs:field xpath="@compartment"/>
                </xs:unique>
              </xs:element>
              <xs:element name="ListOfReactions" id="copasi.Model.ListOfReactions" minOccurs="0">
                <xs:complexType mixed="true">
                  <xs:sequence>
                    <xs:element name="Reaction" id="copasi.Model.Reaction" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="ListOfSubstrates" minOccurs="0">
                            <xs:complexType mixed="true">
                              <xs:sequence>
                                <xs:element name="Substrate" type="cREACTION_ELEMENT" id="copasi.Model.Reaction.Substrate" maxOccurs="unbounded"/>
                              </xs:sequence>
                            </xs:complexType>
                            <xs:unique name="SubstrateName">
                              <xs:selector xpath="Substrate"/>
                              <xs:field xpath="@metabolite"/>
                            </xs:unique>
                          </xs:element>
                          <xs:element name="ListOfProducts" id="copasi.Model.Reaction.ListOfProducts" minOccurs="0">
                            <xs:complexType mixed="true">
                              <xs:sequence>
                                <xs:element name="Product" type="cREACTION_ELEMENT" id="copasi.Model.Reaction.Product" maxOccurs="unbounded"/>
                              </xs:sequence>
                            </xs:complexType>
                            <xs:unique name="ProductName">
                              <xs:selector xpath="Product"/>
                              <xs:field xpath="@metabolite"/>
                            </xs:unique>
                          </xs:element>
                          <xs:element name="ListOfModifiers" id="copasi.Model.Reaction.ListOfModifiers" minOccurs="0">
                            <xs:complexType mixed="true">
                              <xs:sequence>
                                <xs:element name="Modifier" type="cREACTION_ELEMENT" id="copasi.Model.Reaction.Modifier" maxOccurs="unbounded"/>
                              </xs:sequence>
                            </xs:complexType>
                            <xs:unique name="ModifierName">
                              <xs:selector xpath="Modifier"/>
                              <xs:field xpath="@metabolite"/>
                            </xs:unique>
                          </xs:element>
                          <xs:element name="ListOfConstants" id="copasi.Model.Reaction.ListOfConstants" minOccurs="0">
                            <xs:complexType mixed="true">
                              <xs:sequence>
                                <xs:element name="Constant" type="cCONSTANT" id="copasi.Model.Reaction.Constant" maxOccurs="unbounded"/>
                              </xs:sequence>
                            </xs:complexType>
                            <xs:unique name="ConstantName">
                              <xs:selector xpath="Constant"/>
                              <xs:field xpath="@name"/>
                            </xs:unique>
                          </xs:element>
                          <xs:element name="KineticLaw" type="cFUNCTION_CALL" minOccurs="0"/>
                        </xs:sequence>
                        <xs:attribute name="key" type="cREACTION_KEY" use="required" id="copasi.Model.Reaction.key"/>
                        <xs:attribute name="name" type="xs:string" use="required" id="copasi.Model.Reaction.name"/>
                        <xs:attribute name="compartment" type="cCOMPARTMENT_KEYREF" use="optional" default="Compartment_00" id="copasi.Model.Reaction.compartment"/>
                        <xs:attribute name="reversible" type="xs:boolean" use="required" id="copasi.Model.Reaction.reversible"/>
                        <xs:attribute name="sbmlid" type="xs:string" use="optional" id="copasi.Model.Reaction.sbmlid"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
                <xs:unique name="ReactionName">
                  <xs:selector xpath="Reaction"/>
                  <xs:field xpath="@name"/>
                  <xs:field xpath="@compartment"/>
                </xs:unique>
              </xs:element>
              <xs:element name="StateTemplate" id="copasi.Model.StateTemplate" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="StateTemplateVariable" id="copasi.STATE_TEMPLATE.StateTemplateVariable" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="key" type="cSTATE_VARIABLE_KEY" use="required" id="copasi.STATE_TEMPLATE.StateTemplateVariable.key"/>
                        <xs:attribute name="objectReference" type="cKEYREF" use="required" id="copasi.STATE_TEMPLATE.StateTemplateVariable.objectReference"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
                <xs:unique name="StateTemplateVariableObjectReference">
                  <xs:selector xpath="StateTemplateVariable"/>
                  <xs:field xpath="@objectReference"/>
                </xs:unique>
              </xs:element>
              <xs:element name="InitialState" id="copasi.Model.InitialState" minOccurs="0">
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="cSTATE_TYPE"/>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="key" type="cMODEL_KEY" use="required" id="copasi.Model.key"/>
            <xs:attribute name="name" type="xs:string" use="required" id="copasi.Model.name"/>
            <xs:attribute name="timeUnit" use="required" id="copasi.Model.timeUnit">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="d"/>
                  <xs:enumeration value="h"/>
                  <xs:enumeration value="m"/>
                  <xs:enumeration value="s"/>
                  <xs:enumeration value="ms"/>
                  <xs:enumeration value="µs"/>
                  <xs:enumeration value="ns"/>
                  <xs:enumeration value="ps"/>
                  <xs:enumeration value="fs"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="volumeUnit" use="required" id="copasi.Model.volumeUnit">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="m³"/>
                  <xs:enumeration value="l"/>
                  <xs:enumeration value="ml"/>
                  <xs:enumeration value="µl"/>
                  <xs:enumeration value="nl"/>
                  <xs:enumeration value="pl"/>
                  <xs:enumeration value="fl"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="quantityUnit" use="required" id="copasi.Model.quantityUnit">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="Mol"/>
                  <xs:enumeration value="mMol"/>
                  <xs:enumeration value="µMol"/>
                  <xs:enumeration value="nMol"/>
                  <xs:enumeration value="pMol"/>
                  <xs:enumeration value="fMol"/>
                  <xs:enumeration value="#"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="stateVariable" type="cSTATE_VARIABLE_KEYREF" use="required" id="copasi.Model.stateVariable"/>
          </xs:complexType>
        </xs:element>
        <xs:element name="ListOfTasks" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The list of tasks defined for  the current model. If there are no tasks defined this element must not occur.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Task" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Report" type="cREPORT_INSTANTIATION" minOccurs="0"/>
                    <xs:element name="Problem">
                      <xs:complexType>
                        <xs:complexContent>
                          <xs:extension base="cPARAMETER_LIST"/>
                        </xs:complexContent>
                      </xs:complexType>
                      <xs:unique name="ProblemParameterName">
                        <xs:selector xpath="*"/>
                        <xs:field xpath="@name"/>
                      </xs:unique>
                    </xs:element>
                    <xs:element name="Method">
                      <xs:complexType>
                        <xs:complexContent>
                          <xs:extension base="cPARAMETER_LIST">
                            <xs:attribute name="name" type="xs:string" use="required"/>
                            <xs:attribute name="type" type="xs:string" use="optional" default="default"/>
                          </xs:extension>
                        </xs:complexContent>
                      </xs:complexType>
                      <xs:unique name="MethodParameterName">
                        <xs:selector xpath="Parameter"/>
                        <xs:field xpath="@name"/>
                      </xs:unique>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="key" type="cTASK_KEY" use="required"/>
                  <xs:attribute name="name" type="xs:string" use="required"/>
                  <xs:attribute name="type" type="cTASK_TYPE" use="required"/>
                  <xs:attribute name="scheduled" type="xs:boolean" use="optional" default="false"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="ListOfReports" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Report" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Comment" type="cCOMMENT" minOccurs="0"/>
                    <xs:choice>
                      <xs:element name="Table">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element name="Object" type="cOBJECT" minOccurs="0" maxOccurs="unbounded"/>
                          </xs:sequence>
                          <xs:attribute name="printTitle" type="xs:boolean" use="optional" default="true"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:sequence>
                        <xs:element name="Header" type="cREPORT_SECTION" minOccurs="0"/>
                        <xs:element name="Body" type="cREPORT_SECTION" minOccurs="0"/>
                        <xs:element name="Footer" type="cREPORT_SECTION" minOccurs="0"/>
                      </xs:sequence>
                    </xs:choice>
                  </xs:sequence>
                  <xs:attribute name="key" type="cREPORT_KEY" use="required"/>
                  <xs:attribute name="name" type="xs:string" use="required"/>
                  <xs:attribute name="taskType" type="cTASK_TYPE" use="required"/>
                  <xs:attribute name="separator" type="xs:string" use="optional" default=" &#x09;"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
          <xs:unique name="ReportName">
            <xs:selector xpath="Rpeort"/>
            <xs:field xpath="@name"/>
          </xs:unique>
        </xs:element>
        <xs:element name="ListOfPlots" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="PlotSpecification" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:complexContent>
                    <xs:extension base="cPLOTITEM">
                      <xs:sequence>
                        <xs:element name="ListOfPlotItems">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="PlotItem" type="cPLOTITEM" maxOccurs="unbounded"/>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                      <xs:attribute name="active" type="xs:boolean" use="optional" default="true"/>
                    </xs:extension>
                  </xs:complexContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="GUI" id="copasi.GUI" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ListOfSliders" id="copasi.GUI.ListOfSliders" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Slider" id="copasi.GUI.ListOfSliders.Sliders" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="key" type="cSLIDER_KEY" use="required"/>
                        <xs:attribute name="associatedEntityKey" type="cKEYREF" use="required"/>
                        <xs:attribute name="objectCN" type="xs:string" use="required" id="copasi.GUI.ListOfSliders.Sliders.objectCN"/>
                        <xs:attribute name="objectType" use="required" id="copasi.GUI.ListOfSliders.Sliders.objectType">
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:enumeration value="float"/>
                              <xs:enumeration value="unsignedFloat"/>
                              <xs:enumeration value="integer"/>
                              <xs:enumeration value="unsignedInteger"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="objectValue" type="xs:double" use="required" id="copasi.GUI.ListOfSliders.Sliders.objectValue"/>
                        <xs:attribute name="minValue" type="xs:double" use="required" id="copasi.GUI.ListOfSliders.Sliders.minValue"/>
                        <xs:attribute name="maxValue" type="xs:double" use="required" id="copasi.GUI.ListOfSliders.Sliders.maxValue"/>
                        <xs:attribute name="tickNumber" type="xs:positiveInteger" use="optional" default="1000"/>
                        <xs:attribute name="tickFactor" type="xs:positiveInteger" use="optional" default="100"/>
                        <xs:attribute name="scaling" use="optional" default="linear">
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:enumeration value="linear"/>
                              <xs:enumeration value="logarithmic"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="updateRanges" type="xs:boolean" use="optional" default="true"/>
                  <xs:attribute name="autoExecuteTask" type="xs:boolean" use="optional" default="true"/>
                </xs:complexType>
                <xs:unique name="SliderObject">
                  <xs:selector xpath="Slider"/>
                  <xs:field xpath="@objectCN"/>
                </xs:unique>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="versionMajor" type="xs:integer"/>
      <xs:attribute name="versionMinor" type="xs:integer"/>
    </xs:complexType>
    <xs:unique name="ModelStateVariableReference">
      <xs:selector xpath="Model| Model/*/*"/>
      <xs:field xpath="@stateVariable"/>
    </xs:unique>
  </xs:element>
  <xs:complexType name="cCOMMENT" id="copasi.COMMENT">
    <xs:sequence>
      <xs:any namespace="http://www.w3.org/1999/xhtml" processContents="skip" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="cCONSTANT" id="copasi.CONSTANT">
    <xs:attribute name="key" type="cCONSTANT_KEY" use="required"/>
    <xs:attribute name="name" type="xs:string" use="required"/>
    <xs:attribute name="value" type="xs:double" use="required"/>
  </xs:complexType>
  <xs:complexType name="cREACTION_ELEMENT" id="copasi.REACTION_ELEMENT">
    <xs:attribute name="metabolite" type="cMETABOLITE_KEYREF" use="required" id="copasi.REACTION_ELEMENT.metabolite"/>
    <xs:attribute name="stoichiometry" type="xs:double" use="required" id="copasi.REACTION_ELEMENT.stoichiometry"/>
  </xs:complexType>
  <xs:complexType name="cFUNCTION" id="copasi.FUNCTION">
    <xs:sequence>
      <xs:element name="MathML" type="cMATH" id="copasi.FUNCTION.MathML"/>
      <xs:element name="ListOfParameterDescriptions" id="copasi.FUNCTION.ListOfParameterDescriptions" minOccurs="0">
        <xs:complexType mixed="true">
          <xs:sequence>
            <xs:element name="ParameterDescription" type="cFUNCTION_PARAMETER" id="copasi.FUNCTION.ParameterDescription" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
        <xs:unique name="ParameterDescriptionName">
          <xs:selector xpath="ParameterDescription"/>
          <xs:field xpath="@name"/>
        </xs:unique>
        <xs:unique name="ParameterDescriptionOrder">
          <xs:selector xpath="ParameterDescription"/>
          <xs:field xpath="@order"/>
        </xs:unique>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="key" type="cFUNCTION_KEY" use="required" id="copasi.FUNCTION.key"/>
    <xs:attribute name="name" type="xs:string" use="required" id="copasi.FUNCTION.name"/>
    <xs:attribute name="type" use="required" id="copasi.FUNCTION.type">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="Function"/>
          <xs:enumeration value="Expression"/>
          <xs:enumeration value="MassAction"/>
          <xs:enumeration value="PreDefinedKineticLaw"/>
          <xs:enumeration value="UserDefinedKineticLaw"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="positive" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
          <xs:enumeration value="unspecified"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="sbmlid" type="xs:string" use="optional" id="copasi.FUNCTION.sbmlid"/>
  </xs:complexType>
  <xs:complexType name="cFUNCTION_PARAMETER" id="copasi.FUNCTION_PARAMETER">
    <xs:attribute name="key" type="cFUNCTION_PARAMETER_KEY" use="required" id="copasi.FUNCTION_PARAMETER.key"/>
    <xs:attribute name="name" type="xs:string" use="required" id="copasi.FUNCTION_PARAMETER.name"/>
    <xs:attribute name="order" type="xs:nonNegativeInteger" use="required" id="copasi.FUNCTION_PARAMETER.order"/>
    <xs:attribute name="role" use="required" id="copasi.FUNCTION_PARAMETER.role">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="substrate"/>
          <xs:enumeration value="product"/>
          <xs:enumeration value="modifier"/>
          <xs:enumeration value="constant"/>
          <xs:enumeration value="other"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="minOccurs" type="xs:nonNegativeInteger" use="optional" default="1" id="copasi.FUNCTION_PARAMETER.minOccurs"/>
    <xs:attribute name="maxOccurs" use="optional" default="1" id="copasi.FUNCTION_PARAMETER.maxOccurs">
      <xs:simpleType>
        <xs:union>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="unbounded"/>
            </xs:restriction>
          </xs:simpleType>
          <xs:simpleType>
            <xs:restriction base="xs:nonNegativeInteger"/>
          </xs:simpleType>
        </xs:union>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="cFUNCTION_CALL" id="copasi.FUNCTION_CALL">
    <xs:sequence>
      <xs:element name="ListOfCallParameters" id="copasi.FUNCTION.ListOfCallParameters" minOccurs="0">
        <xs:complexType mixed="true">
          <xs:sequence>
            <xs:element name="CallParameter" type="cFUNCTION_CALL_PARAMETER" id="copasi.FUNCTION_CALL.ListOfCallParameters.CallParameter" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
        <xs:unique name="CallParameterName">
          <xs:selector xpath="CallParameter"/>
          <xs:field xpath="@functionParameter"/>
        </xs:unique>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="function" type="cFUNCTION_KEYREF" use="required" id="copasi.FUNCTION_CALL.function"/>
  </xs:complexType>
  <xs:complexType name="cFUNCTION_CALL_PARAMETER" id="copasi.FUNCTION_CALL_PARAMETER">
    <xs:sequence>
      <xs:element name="SourceParameter" id="copasi.FUNCTION_CALL_PARAMETER.SourceParameter" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attribute name="reference" type="cKEYREF" use="required" id="copasi.FUNCTION_CALL_PARAMETER.SourceParameter.reference"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="functionParameter" type="cFUNCTION_PARAMETER_KEYREF" use="required" id="copasi.FUNCTION_CALL_PARAMETER.functionParameter"/>
  </xs:complexType>
  <xs:complexType name="cMATH">
    <xs:choice>
      <xs:sequence>
        <xs:any namespace="http://www.w3.org/Math/XMLSchema" processContents="skip" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:element name="Text">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="cOBJECT">
    <xs:attribute name="cn" type="xs:string" use="required"/>
  </xs:complexType>
  <xs:simpleType name="cSTATE">
    <xs:list itemType="xs:double"/>
  </xs:simpleType>
