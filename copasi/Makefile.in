apidox: apidox-am
	@set fnord $(MAKEFLAGS); amf=$$2; \
	list='$(SUBDIRS)'; for subdir in $$list; do \
		echo "Making apidox in $$subdir"; \
		if test "$$subdir" != "."; then \
			(cd $$subdir && $(MAKE) $(AM_MAKEFLAGS) apidox) || exit 1; \
		fi ;\
	done

#>+ 6
force-reedit:
		cd $(top_srcdir) && \
	$(AUTOMAKE) --gnu  copasi/Makefile
	cd $(top_srcdir) && perl admin/am_edit copasi/Makefile.in


#>+ 11
copasi.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/main.cpp
	@echo 'creating copasi.all_cpp.cpp ...'; \
	rm -f copasi.all_cpp.files copasi.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> copasi.all_cpp.final; \
	for file in main.cpp ; do \
	  echo "#include \"$$file\"" >> copasi.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> copasi.all_cpp.final; \
	done; \
	cat copasi.all_cpp.final copasi.all_cpp.files > copasi.all_cpp.cpp; \
	rm -f copasi.all_cpp.final copasi.all_cpp.files

